pipeline {
	agent any
	
	stages {
		stage ('CLEANING STAGE') {
			steps {
				echo "******************************"
				echo "REMOVING OBJECT AND EXE FILES"
				echo "******************************"
				sh 'make clean'
			}
		}
		stage ('BUILD') {
			steps {
				echo "*****************************"
				echo "CREATING EXE FILES"
				echo "*****************************"
				sh 'build.sh'
			}
		}

	}
}
